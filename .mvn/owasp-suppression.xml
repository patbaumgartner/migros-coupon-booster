<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

  <!-- Suppress false positives for test dependencies -->
  <suppress>
    <notes><![CDATA[
    Suppress test-related dependencies that may have vulnerabilities but are not used in production
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.springframework/spring\-test@.*$</packageUrl>
    <cve>CVE-2024-22243</cve>
  </suppress>

  <!-- Suppress known issues with Playwright that are not exploitable in our context -->
  <suppress>
    <notes><![CDATA[
    Playwright browser automation tool - CVEs related to browser engines not applicable to our use case
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/com\.microsoft\.playwright/playwright@.*$</packageUrl>
    <vulnerabilityName regex="true">.*browser.*</vulnerabilityName>
  </suppress>

  <!-- Add more suppressions as needed based on your specific use case -->
</suppressions>
